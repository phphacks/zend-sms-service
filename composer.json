{
    "name": "phphacks/zend-sms-service",
    "description": "Adapter to use any sms provider in your zend application",
    "type": "library",
    "require": {
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-mvc": "^3.1",
        "comtele/comtele_sdk": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
	"psr-4": {
	   "Zend\\SMSService\\": "src"
	}
    },
    "autoload-dev": {
	"psr-4": {
	   "Zend\\SMSService\\Tests\\": "tests"
	}
    },
    "license": "MIT",
    "authors": [
        {
            "name": "JC Fachini",
            "email": "joao.carlos@bevicred.com.br"
        }
    ],
    "minimum-stability": "stable"
